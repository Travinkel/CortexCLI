{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Counterexample Generation Atom",
  "description": "Provide a counterexample to a claim.",
  "type": "object",
  "required": [
    "prompt",
    "claim",
    "counterexample",
    "explanation"
  ],
  "properties": {
    "prompt": {
      "type": "string"
    },
    "claim": {
      "type": "string"
    },
    "counterexample": {
      "type": "string"
    },
    "explanation": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
